  "panels": [
    {
      "title": "Conservation Health",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "rate(finn_conservation_violations_total[5m])",
          "legendFormat": "violations/s"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 0.01 }
            ]
          },
          "unit": "ops"
        }
      }
    },
    {
      "title": "Conservation Violations (5m rate)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "targets": [
        {
          "expr": "rate(finn_conservation_violations_total[5m])",
          "legendFormat": "violations/s"
        },
        {
          "expr": "rate(finn_conservation_checks_total[5m])",
          "legendFormat": "checks/s"
        }
      ]
    },
    {
      "title": "Credit Flow",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "targets": [
        {
          "expr": "finn_credits_by_state{state=\"available\"}",
          "legendFormat": "available"
        },
        {
          "expr": "finn_credits_by_state{state=\"reserved\"}",
          "legendFormat": "reserved"
        },
        {
          "expr": "finn_credits_by_state{state=\"settled\"}",
          "legendFormat": "settled"
        },
        {
          "expr": "finn_escrow_balance_total",
          "legendFormat": "escrow"
        }
      ]
    },
    {
      "title": "Payment Method Breakdown",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 6, "x": 0, "y": 12 },
      "targets": [
        {
          "expr": "sum by (payment_method) (finn_agent_requests_total)",
          "legendFormat": "{{payment_method}}"
        }
      ]
    },
    {
      "title": "Agent Usage by Archetype",
      "type": "bargauge",
      "gridPos": { "h": 8, "w": 6, "x": 6, "y": 12 },
      "targets": [
        {
          "expr": "sum by (archetype) (finn_agent_requests_total)",
          "legendFormat": "{{archetype}}"
        }
      ]
    },
    {
      "title": "x402 Verification Success Rate",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 12 },
      "targets": [
        {
          "expr": "sum(rate(finn_x402_verifications_total{result=\"success\"}[5m])) / sum(rate(finn_x402_verifications_total[5m]))",
          "legendFormat": "success rate"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "thresholds": {
            "steps": [
              { "color": "red", "value": null },
              { "color": "yellow", "value": 0.5 },
              { "color": "green", "value": 0.95 }
            ]
          }
        }
      }
    },
    {
      "title": "x402 Verifications (5m rate)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 16 },
      "targets": [
        {
          "expr": "rate(finn_x402_verifications_total{result=\"success\"}[5m])",
          "legendFormat": "success"
        },
        {
          "expr": "rate(finn_x402_verifications_total{result=\"failure\"}[5m])",
          "legendFormat": "failure"
        },
        {
          "expr": "rate(finn_x402_verifications_total{result=\"replay\"}[5m])",
          "legendFormat": "replay"
        }
      ]
    },
    {
      "title": "RPC Health by Provider",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 20 },
