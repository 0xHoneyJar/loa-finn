        {
          "expr": "rate(finn_rpc_requests_total{result=\"success\"}[5m])",
          "legendFormat": "{{provider}} success"
        },
        {
          "expr": "rate(finn_rpc_requests_total{result=\"error\"}[5m])",
          "legendFormat": "{{provider}} error"
        }
      ]
    },
    {
      "title": "Request Latency p50/p95/p99",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 20 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(finn_request_duration_seconds_bucket[5m]))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, rate(finn_request_duration_seconds_bucket[5m]))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, rate(finn_request_duration_seconds_bucket[5m]))",
          "legendFormat": "p99"
