        {
          "expr": "rate(finn_settlement_total{status=\"failure\"}[5m])",
          "legendFormat": "failure"
        }
      ]
    },
    {
      "title": "Rate Limit Hits",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 28 },
