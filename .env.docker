# Required (set real values)
ANTHROPIC_API_KEY=sk-ant-PLACEHOLDER

# PostgreSQL
POSTGRES_PASSWORD=postgres_dev
FINN_APP_PASSWORD=finn_app_dev
FINN_MIGRATE_PASSWORD=finn_migrate_dev
FINN_POSTGRES_ENABLED=true

# Agent
